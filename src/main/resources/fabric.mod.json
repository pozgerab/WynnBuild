{
  "schemaVersion": 1,
  "id": "quickbuild",
  "version": "${version}",
  "name": "QuickBuild",
  "description": "Mod for generating a wynnbuilder url with just one command\nConfig is opened with /build config",
  "authors": [
    "Gertoxq"
  ],
  "contact": {
    "homepage": "https://github.com/pozgerab/WynnBuild",
    "sources": "https://github.com/pozgerab/WynnBuild",
    "issues": "https://github.com/pozgerab/WynnBuild/issues"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.github_releases": "https://github.com/pozgerab/WynnBuild/releases",
        "modmenu.modrinth": "https://modrinth.com/mod/wynnbuild"
      }
    }
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/quickbuild/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.gertoxq.quickbuild.client.QuickBuildClient"
    ],
    "main": [
      "com.gertoxq.quickbuild.QuickBuild"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
